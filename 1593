#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int is_zero(char *str) {
    int len = strlen(str);
    for (int i = 0; i < len; i++) {
        if (str[i] != '0') {
            return 0;
        }
    }
    return 1;
}

int divide_by_two(char *str) {
    int len = strlen(str);
    int remainder = 0;
    char result[1001] = {0}; 
    int j = 0;
    int started = 0;
    
    for (int i = 0; i < len; i++) {
        int current = remainder * 10 + (str[i] - '0');
        if (current / 2 > 0 || started) {
            result[j++] = (current / 2) + '0';
            started = 1;
        }
        remainder = current % 2;
    }
    
    
    if (j == 0) {
        result[0] = '0';
        result[1] = '\0';
    } else {
        result[j] = '\0';
    }
    
    strcpy(str, result);
    return remainder;
}

int main() {
    int t;
    char num[1001]; 
    
    scanf("%d", &t);
    
    for (int i = 0; i < t; i++) {
        scanf("%s", num);
        
        int ans = 0;
        
        while (!is_zero(num)) {
            int bit = divide_by_two(num);
            if (bit == 1) {
                ans++;
            }
        }
        
        printf("%d\n", ans);
    }
    
    return 0;
}
